using System;

namespace libroJava
{
    class Program
    {

        static void Main(string[] args)
        { int num;

            Console.WriteLine("esto es una tabla de multiplicar");
            num = int.Parse(Console.ReadLine());
            for (int i = 0; i <= 10; i++)
            {
                Console.Write("la tabla de multiplicar de "+num+" es...."+num+"x"+i+"=");
                Console.WriteLine(num*i  );
            }
    
        }

    }
}
